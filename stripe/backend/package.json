    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
