  "version": "1.0.0",
  "description": "Stripe-like payment processing backend",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --watch src/index.js",
    "dev:server1": "PORT=3001 npm run dev",
