{
  "name": "distributed-cache-backend",
  "version": "1.0.0",
  "description": "A distributed cache server with consistent hashing, LRU eviction, and TTL support",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "dev:server1": "PORT=3001 NODE_ID=node-1 node --watch src/server.js",
    "dev:server2": "PORT=3002 NODE_ID=node-2 node --watch src/server.js",
    "dev:server3": "PORT=3003 NODE_ID=node-3 node --watch src/server.js",
    "start": "node src/server.js",
    "coordinator": "PORT=3000 node --watch src/coordinator.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "cache",
    "distributed",
    "consistent-hashing",
    "lru",
    "ttl"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "morgan": "^1.10.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  }
}
